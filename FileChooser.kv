<FileChooserGUI>:
    BoxLayout:
        orientation:'vertical'
        padding:[10,10,10,10]
        spacing: 10
        FileChooserListView
            id:file_chooser
            dirselect: True
            fileselect: False
            on_selection:root.select_to(*args)
        Button:
            size_hint: (1, 0.1)
            text:'Select'
